alias l="ls -lAh --color=auto"
alias c="clear"
alias s="cd ~/Scripts"
alias ..="cd .."
alias ..2="cd ../.."
alias ..3="cd ../../.."
alias ..4="cd ../../../.."
alias ..5="cd ../../../../.."
stty -ixon -ixoff 
